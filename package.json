{
  "name": "twitter-cleaner",
  "displayName": "twitter-cleaner",
  "version": "0.1.0",
  "description": "A browser extension that allows you to clean up your Twitter feed by getting rid of unwanted replies on Twitter. Say goodbye to onlyfans links, engagement bait, and more.",
  "author": "Raphael Salaja",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-toolbar": "^1.0.4",
    "animejs": "^3.2.2",
    "@vladmandic/face-api": "^1.7.13",
    "nsfwjs": "^4.1.0",
    "bip39": "^3.1.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "lucide-react": "^0.363.0",
    "plasmo": "0.85.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwind-merge": "^2.2.2",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@radix-ui/colors": "2.1.0",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "postcss": "8.4.33",
    "prettier": "3.2.4",
    "tailwindcss": "3.4.1",
    "typescript": "5.3.3",
    "windy-radix-palette": "0.6.1"
  },
  "manifest": {
    "permissions": [
      "activeTab",
      "<all_urls>",
      "scripting"
    ],
    "web_accessible_resources": [
      {
        "resources": [
          "models/*"
        ],
        "matches": [
          "<all_urls>"
        ]
      }
    ]
  }
}
